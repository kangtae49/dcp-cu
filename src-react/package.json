{
  "name": "dcp-cu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --outDir ../src-py/gui --emptyOutDir --minify && cd ../src-py && uv run pyinstaller build-windows.spec",
    "lint": "eslint .",
    "preview": "vite preview",
    "py": "cd ../src-py && uv run main.py --verbose",
    "models": "cd ../src-py && uv run pydantic2ts --module apps/models.py --output ../src-react/src/types/models.d.ts",
    "start-redux-devtools": "redux-devtools --hostname=localhost --port=8001"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@reduxjs/toolkit": "^2.9.2",
    "@silevis/reactgrid": "^4.1.17",
    "@szhsin/react-menu": "^4.5.0",
    "classnames": "^2.5.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "natsort": "^2.0.3",
    "react": "^19.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "react-jdenticon": "^1.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.5",
    "react-virtualized-auto-sizer": "^1.0.26",
    "recharts": "^3.5.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@redux-devtools/cli": "^4.0.4",
    "@redux-devtools/remote": "^0.9.5",
    "@types/lodash": "^4.17.21",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "mime-types": "^3.0.1",
    "prop-types": "^15.8.1",
    "redux": "^5.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
